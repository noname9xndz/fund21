        .user-faq-filter-category
            span Danh mục:
            ul
                li.is-active
                    a(href="") Tất cả
                li
                    a(href="") Bắt đầu với SaveNow
                li
                    a(href="") Khẩu vị rủi ro
                li
                    a(href="") Danh mục đầu tư
                li
                    a(href="") Đầu tư và rút tiền
        .user-faq-filter-pagesize
            span Hiển thị
            select.user-faq-filter-pagesize__select.table-filter-actions__pagesize__select#faq-pagesize(name="faq-pagesize")
                option(value="10") 10
                option(value="15") 15
                option(value="20") 20
